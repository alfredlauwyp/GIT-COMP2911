import java.util.Comparator;


public class AsearchComparator implements Comparator
{
    public int compare(SearchNode e1, SearchNode e2)
    { 
    	if (e1.getExternalDistanceTravelled() + heuristic.getEstimate(e1)
    	 > e2.getExternalDistanceTravelled() + heuristic.getEstimate(e2)) return 1;
    	if (e1.getExternalDistanceTravelled() + heuristic.getEstimate(e1)
		 < e2.getExternalDistanceTravelled() + heuristic.getEstimate(e2)) return -1;
    	else return 0;
    }
}
