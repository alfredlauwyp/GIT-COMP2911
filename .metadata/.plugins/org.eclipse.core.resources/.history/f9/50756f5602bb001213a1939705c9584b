import java.util.LinkedList;
import java.awt.Point;

public class JobPoint<E>
{

	public JobPoint(E fromX, E fromY, E toX, E toY)
	{
		LinkedList<E> from = new LinkedList<E>();
		from.add(INDEX_X, fromX);
		from.add(INDEX_Y, fromY);
		
		LinkedList<E> to = new LinkedList<E>();
		from.add(INDEX_X, toX);
		from.add(INDEX_Y, toY);
		
		jobs = new LinkedList<LinkedList<E>>();
		jobs.add(INDEX_FROM, from);
		jobs.add(INDEX_TO, to);
	}
	
	public E getFromX()
	{
		return pointFrom.get(INDEX_X);
	}
	
	private LinkedList<E> pointFrom()
	{
		return jobs.get(INDEX_FROM);
	}
	
	private LinkedList<E> pointTo()
	{
		return jobs.get(INDEX_TO);
	}
	
	private LinkedList<LinkedList<E>> jobs;
	private static final int INDEX_FROM = 0;
	private static final int INDEX_TO = 1;
	private static final int INDEX_X = 0;
	private static final int INDEX_Y = 1;
	
	
	
}