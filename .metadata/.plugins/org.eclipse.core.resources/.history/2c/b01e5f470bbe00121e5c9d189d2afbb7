
public class Comparator {
	
	MinimalEdgeHeuristic<SearchNode> heuristic = new MinimalEdgeHeuristic<SearchNode>();
	public int compare(SearchNode e1, SearchNode e2) { 
		if (e1.getExternalDistanceTravelled() + heuristic.getEstimate(e1, minimalDistance)
	     > e2.getExternalDistanceTravelled() + heuristic.getEstimate(e2, minimalDistance)) return 1;
	    if (e1.getExternalDistanceTravelled() + heuristic.getEstimate(e1, minimalDistance)
		 < e2.getExternalDistanceTravelled() + heuristic.getEstimate(e2, minimalDistance)) return -1;
	    else return 0;
	}
}
