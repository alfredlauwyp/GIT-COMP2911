import java.util.LinkedList;

public interface SearchNode<JobPoint> {

	public boolean equals(Object nodeObj);
	public JobPoint getNodeObj();
	public void addVisited(AsearchNode<JobPoint> newVisited);
	public int getNumNodesVisited();
	public boolean hasVisited(JobPoint otherNode);
	public LinkedList<AsearchNode<JobPoint>> getNodesVisited();
	public LinkedList<JobPoint> getNodeObjsVisited();
	public int getDistanceTravelled();
	
}