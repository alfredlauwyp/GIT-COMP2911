import java.util.ArrayList;

public class AdjListGraphTester {

	public static void main(String argv[])
	{
		Graph<String> g = new AdjListGraph<String>();
		g.addNode("H");
		g.addNode("Bye");
		
		System.out.println("Is H in graph? " + g.isInGraph("H"));
        System.out.println("Is A in graph? " + g.isInGraph("A"));
		System.out.println("Is Bye in graph? " + g.isInGraph("Bye"));
		System.out.println("\n");
		
		System.out.println("Number of vertices: " + g.getNumVertices());
		g.printGraph();
		//ArrayList<ArrayList<String>> objs = g.getNodes();
		//String s = objs.get(0).get(0);
		//System.out.println(s);
	}
}
